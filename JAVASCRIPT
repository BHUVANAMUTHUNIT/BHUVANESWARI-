<head>
	<title>Window location</title>
	<script type="text/javascript">
		function show()
		{
			var un=document.getElementById("t1").value;
			var pw=document.getElementById("t2").value;
			if(un=="bhuvana" && pw=="muthu")
			{
				window.location.href="set time out.html";
			}
			else
			{
				document.getElementById("res").innerHTML="<font size='7' color='red'>Invalid Credential!!</font>";
			}
		}
	</script>
