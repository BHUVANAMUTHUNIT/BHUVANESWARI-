CONTROL STATEMENTS:
==================
<!DOCTYPE html>
<html>
<head>
	<title>Control Statements</title>
	<script type="text/javascript">
		function dis() 
		{
			var k1=parseInt(document.getElementById("t1").value);
			var k2=parseInt(document.getElementById("t2").value);
			var k3=parseInt(document.getElementById("t3").value);
			var k4=parseInt(document.getElementById("t4").value);
			var k5=parseInt(document.getElementById("t5").value);
			var tot=k1+k2+k3+k4+k5;
			var avg=tot/5;
			var result=" ",grade=" ";
			if(k1>34 && k2>34 && k3>34 && k4>34 && k5>34)
			{
				result="PASS";
				if(avg>=85)
					grade="outstanding";
				else if(avg>=75)
					grade="excellent";
				else if(avg>=65)
					grade="very good";
				else if(avg>=55)
					grade="good";
				else
					grade="fair";
			}
			else
			{
				result="fail";
				grade="*******";
			}
			document.getElementById("res1").innerHTML="Total:"+tot;
			document.getElementById("res2").innerHTML="Percentage:"+avg;
			document.getElementById("res3").innerHTML="result:"+result;
			document.getElementById("res4").innerHTML="grade:"+grade;
		}
	</script>
</head>
<body>
	<form>
		<input id="t1" type="text" placeholder="Enter tamil mark:"><br>
		<input id="t2" type="text" placeholder="Enter english mark:"><br>
		<input id="t3" type="text" placeholder="Enter maths mark:"><br>
		<input id="t4" type="text" placeholder="Enter science mark:"><br>
		<input id="t5" type="text" placeholder="Enter social mark:"><br>
		<input type="button" value="Find Total" onclick="dis()">
		<div id="res1"></div>
		<div id="res2"></div>
		<div id="res3"></div>
		<div id="res4"></div>
		
	</form>

</body>
</html>

