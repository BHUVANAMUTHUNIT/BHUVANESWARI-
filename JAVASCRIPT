	<title>Relational Operator 2</title>
	<script type="text/javascript">
		 
	    var d="reethika"<"bhuvana";
		document.write("<br> d value:"+d);
		var d="bhuvana"<="reethika";
		document.write("<br> d value:"+d);
		var d="muthu"=="muthu";
		document.write("<br> d value:"+d);
		var d="nithulya"!="nithulya";
		document.write("<br> d value:"+d);
		var d="sri">"subi";
		document.write("<br> d value:"+d);
		var d="subi">="sri";
		document.write("<br> d value:"+d);
