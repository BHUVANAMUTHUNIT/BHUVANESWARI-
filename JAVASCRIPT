		document.write("("+a+"+"+b+")<sup>2</sup>="+a+"<sup>2</sup>+"+b+"<sup>2</sup>+2("+a+"*"+b+")");
	   var ans=(a*a)+(b*b)+2*(a*b);
	   document.write("<br> answer="+ans);
