EB-BILL 1:
=========
<!DOCTYPE html>
<html>
<head>
	<title>EBbill 2</title>
	<script type="text/javascript">
		function show() 
		{
			var a=document.getElementById("e").value;
			if (a<=100) 
			document.getElementById("res").innerHTML="Amount:Free";
		else if (a>100 && a<=200)
			document.getElementById("res").innerHTML="Amount:"+((a-100)*2);
		else if (a>200 && a<=300)
			document.getElementById("res").innerHTML="Amount:"+((100*2)+((a-200)*3));
       else 
			document.getElementById("res").innerHTML="Amount:"+((100*2)+(100*3)+(a-300)*5);



		}
	</script>
</head>
<body>
	<form>
		<input type="text" id="e" placeholder="Enter unit"><br>
		<input type="button" value="Amount" onclick="show()">
		<div id="res"></div>
	</form>


</body>
</html>

EB-BILL:
=======
<!DOCTYPE html>
<html>
<head>
	<title>EB bill</title>
	<script type="text/javascript">
		function show() 
		{
			var units=parseInt(document.getElementById("t1").value);
			if(units<=100)
				document.getElementById("res").innerHTML="Free";
			else if (units>=101 && units<=200) 
			{
				var amt=(units-100)*2;
				if(amt<=50)
					document.getElementById("res").innerHTML="Amount:Rs.50";
				else
					document.getElementById("res").innerHTML="Amount:Rs."+amt;
			}
			else if (units>=201 && units<=300) 
			{
				var amt=0+200+(units-200)*3;
				document.getElementById("res").innerHTML="Amount:Rs."+amt;
			}
			else 
			{
				var amt=0+200+300+(units-300)*5;
				document.getElementById("res").innerHTML="Amount:Rs."+amt;
			}


		}
	</script>
</head>
<body>
	<form>
		<input type="text" id="t1" placeholder="Enter no. of units:">
		<br>
		<input type="button" value="Total bill Amount" onclick="show()">
		<div id="res"></div>
	</form>

</body>
</html>

