	<title>Specialchar validation</title>
	<script type="text/javascript">
		function dis()
		{
			var data=document.getElementById("t1").value;
			var ans="";
			if(data.match(/^[^a-zA-Z0-9]{5,10}$/))
				ans="accepted";
				else
					ans="not accept special char's";
				document.getElementById("res").innerHTML=ans;
		}
	</script>
</head>
<body>
