	<title>String Inbuilt Function</title>
	<script type="text/javascript">
		function dis1() 
		{
			var t=document.getElementById("t1").value;
			document.getElementById("res1").innerHTML="Length of text:"+t.length;
		}
		function dis2() 
		{
			var t=document.getElementById("t1").value;
			document.getElementById("res2").innerHTML="Upper Case text:"+t.toUpperCase();
		}
		function dis3() 
		{
			var t=document.getElementById("t1").value;
			document.getElementById("res3").innerHTML="Lower Case text:"+t.toLowerCase();
		}
	</script>
</head>
<body>
	<form>
		<input type="text" placeholder="Enter your name" id="t1">
		<br>
		<input type="button" onclick="dis1()" value="Find Length:">
		<input type="button" onclick="dis2()" value="Upper Case:">
		<input type="button" onclick="dis3()" value="Lower Case:">
		<br>
		<div id="res1"></div>
		<div id="res2"></div>
		<div id="res3"></div>
	</form>

</body>
</html>

