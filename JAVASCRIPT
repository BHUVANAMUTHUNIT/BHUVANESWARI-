	<title>Variable Declaration 2</title>
	<script type="text/javascript">
		var a=5;
		var b=7;
		var c=2;
		var d=(a+b)*c;
		//(10+20)*3=90
		document.write("("+a+"+"+b+")*"+c+"="+d);
		document.write("<br>");
		var e=a+(b*c);
		//10+(20*3)=70
		document.write(a+"+("+b+"*"+c+")="+e);
	</script>
</head>
<body>

</body>
</html>

