		function dis() 
		{
			var k1=document.getElementById("t1").value;
			var k2=document.getElementById("t2").value;
			//alert(k1+" "+k2);
			//document.write("user name:"+k1);
			//document.write("<br>password name:"+k2);
			//console.log("user name:"+k1);
			//console.log("password:"+k2);
			document.getElementById("res").innerHTML="User Details:"+k1+" "+k2;

		}
	</script>
</head>
<body>
	<form>
		<input id="t1" type="text" placeholder="Enter User Name:"><br>
