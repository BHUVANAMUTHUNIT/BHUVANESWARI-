JAVASCRIPT DATE:
===============
<!DOCTYPE html>
<html>
<head>
	<title>Javascript date</title>
	<script type="text/javascript">
		function disdate()
		{
			var d=new Date();
			document.getElementById("res").innerHTML=d;
			setTimeout(disdate,1000);
		}
	</script>
</head>
<body onload="disdate()">
         <div id="res"></div>
</body>
</html>

JAVASCRIPT DATE 1:
=================
<!DOCTYPE html>
<html>
<head>
	<title>Javascript date</title>
	<script type="text/javascript">
		function disdate()
		{
			var d=new Date();
			document.getElementById("res").innerHTML=d;
			var d=new Date();
			var month=d.getMonth();
			var year=d.getFullYear();
			var date=d.getDate();
			document.getElementById("res1").innerHTML=date+"-"+month+"-"+year;
			var d=new Date();
			var h=d.getHours();
			var m=d.getMinutes();
			var s=d.getSeconds();
			document.getElementById("res2").innerHTML=h+":"+m+":"+s;
			if(h<12)
				document.getElementById("res3").innerHTML=h+":"+m+":"+s+"AM";
			else
				document.getElementById("res3").innerHTML=(h-12)+":"+m+":"+s+"PM";
			setTimeout(disdate,1000);
		}
	</script>
</head>
<body onload="disdate()">
         <div id="res"></div>
         <div id="res1"></div>
         <div id="res2"></div>
         <div id="res3"></div>
                 
</body>
</html>

