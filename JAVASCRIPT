		document.write("<br> d value:"+d);
		var d=5.6<=5.6;
		document.write("<br> d value:"+d);
		var d=7.3!=7.3;
		document.write("<br> d value:"+d);
		var d=10.2==10.2;
		document.write("<br> d value:"+d);
		var d=5.3>2.2;
		document.write("<br> d value:"+d);
		var d=6.8>=7.2;
		document.write("<br> d value:"+d);
	</script>
</head>
