JS 5:
====
<!DOCTYPE html>
<html>
<head>
	<title>Js5</title>
	<script type="text/javascript">
		var i=-1;
		var myimg=new Array("1 ice","2 ice","3 ice","double rose","7 ice","90");
		function show()
		{
			i++;
			document.getElementById("iname").innerHTML="<h2>f:/images/"+myimg[i]+".jpg</h2>";
			document.getElementById("imgid").src="f:/images/"+myimg[i]+".jpg";
			if(i==5)
				i=-1;
			setTimeout(show,2000);
		}
	</script>
	<style type="text/css">
		img
		{
			transition-property: width,height;
			transition-duration: 1s;
			transition-timing-function: ease-in-out;
			animation-name: myanimation;
			animation-duration: 2s;
			animation-iteration-count: infinite;
			animation-delay: 0s;
			animation-timing-function: ease-in-out; 
		}
		@keyframes myanimation
		{
			from
			{
				transform: rotate(0deg);
				border-radius: 40%;
				border:10px double red;
				opacity: .0;
				margin-left: 1000px;
			}
			to 
			{
             transform: rotateY(-360deg);
             transform: rotateX(360deg);
             border:10px double red;
             border-radius: 70%;
			 opacity: .9;
			 margin-left: -700px;
			}
		}
		img:hover
		{
			border-radius: 50%;
			width: 50%;
			height: auto;
		}
	</style>
</head>
<body onload="show()">
  <center>
  	<div id="iname">.........</div>
  	<img src="" width="400" height="400" id="imgid">
  </center>
</body>
</html>

