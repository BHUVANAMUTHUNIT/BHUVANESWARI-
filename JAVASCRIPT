	<title>Radio</title>
	<script type="text/javascript">
		function dis()
		{
			var m=document.getElementById("g1").checked;
			var f=document.getElementById("g2").checked;
			var o=document.getElementById("g3").checked;
			if(m==true)
				document.getElementById("res").innerHTML="Male selector";
			else if(f==true)
				document.getElementById("res").innerHTML="FeMale selector";
			else
				document.getElementById("res").innerHTML="others selector";

		}
	</script>
</head>
