function App()
{
  const show=(event)=>{
    document.getElementById("r1").innerHTML="you are using...."+event.target.type
  }
  return(
    <>
    <h1>Event target--type</h1>
    USERNAME:<input type="text" onChange={(e)=>show(e)} placeholder="enter username"></input><br></br>
    PASSWORD:<input type="password" onChange={(e)=>show(e)} placeholder="enter password"></input><br></br>
    EMAIL:<input type="email" onChange={(e)=>show(e)} placeholder="enter email"></input><br></br>
    DATE:<input type="date" onChange={(e)=>show(e)} placeholder="enter date"></input><br></br>
    RANGE:<input type="range" onChange={(e)=>show(e)} min="0" max="10"></input>
    <div id="r1"></div>
    </>
  )
}
