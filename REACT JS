    <input type="checkbox" onChange={(e)=>show(e)}></input>C++
    <input type="checkbox" onChange={(e)=>show(e)}></input>Java
    <input type="checkbox" onChange={(e)=>show(e)}></input>Python
    <input type="checkbox" onChange={(e)=>show(e)}></input>Tally
    <br></br>
    <input type="number" onChange={(e)=>show(e)}></input><br></br>
    <input type="time" onChange={(e)=>show(e)}></input><br></br>
    <input type="week" onChange={(e)=>show(e)}></input><br></br>
    <input type="month" onChange={(e)=>show(e)}></input><br></br>
    <button type="button" onClick={(e)=>show(e)}>clickme</button>
    <div id="r1"></div>
    </>
  )
}
export default App

