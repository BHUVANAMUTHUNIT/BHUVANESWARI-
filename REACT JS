REACT HOOKS USESTATE:
====================
import {useState} from 'react'
function App()
{
  const[name,setName]=useState("****")
  const show=(event)=>{
    setName(event.target.value)
  }
  return(
    <>
    <h1>React textbox issue -we can't change text</h1>
    <h1>problem overcome using hook</h1>
    <input type="text" onChange={(e)=>show(e)} value={name}></input>
    </>
  )
}
export default App

REACT HOOKS USESTATE 1:
======================
import {useState} from 'react'
function App()
{
  const[name,setName]=useState("")
  const[city,setCity]=useState("")
  const show=(event)=>{
    if(event.target.id==="t1")
    setName(event.target.value)
    if(event.target.id==="t2")
    setCity(event.target.value)
  }
  return(
    <>
    <h1>React textbox issue -we can't change text</h1>
    <h1>problem overcome using hook</h1>
    Enter Username:<input type="text" id="t1" onChange={(e)=>show(e)} value={name}></input><br></br>
    Enter City:<input type="text" id="t2" onChange={(e)=>show(e)} value={city}></input>
    </>
  )
}
export default App

REACT HOOKS USEEFFECT AND USESTATE:
==================================
import {useState, useEffect} from 'react'
function App()
{
  const[title,setTitle]=useState("bhuvana")
  useEffect(()=>{
document.title=title
  })
  const show=()=>{
    setTitle("nithu")
  }
return(
  <>
  <h1>UseEffect Hooks</h1>
  <h1>Default title:{title}</h1>
  <button onClick={show}>clickme</button> 
  </>
)
}
export default App

REACT HOOKS USESTATE AND USEEFFECT 1:
====================================
import {useState, useEffect} from 'react'
function App()
{
  const[title,setTitle]=useState("bhuvana")
  var k=0
  useEffect(()=>{
document.title=title + ++k
console.log("use effect",++k)
  })
  const show=(event)=>{
    setTitle(event.target.value)
  }
  const show1=(event)=>{
    event.preventDefault()
  }
return(
  <>
  <h1>UseEffect Hooks</h1>
  <h1>Default title:{title}:{++k}</h1>
  <input type="text" onChange={(e)=>show(e)} value={title} placeholder="enter title to change"></input>
  <button onClick={(e)=>show1(e)}>clickme</button> 
  </>
)
}
export default App

REACT HOOKS USEFORM:
===================
import {useForm} from 'react-hook-form'
function App()
{
const {register,handleSubmit}=useForm()
const show=(data)=>{
  console.log(data)
  document.getElementById("r1").innerHTML=data.un
  document.getElementById("r2").innerHTML=data.pw
  document.getElementById("r3").innerHTML=data.dob
  document.getElementById("r4").innerHTML=data.mark
  document.getElementById("r5").innerHTML=data.gender
}
return(
  <>
  <h1>React Hook Form</h1>
  <form onSubmit={handleSubmit(show)}>
<input type="text" name="un" placeholder='enter user name' {...register("un")}></input><br></br>
<input type="password" name="pw" placeholder='enter password' {...register("pw")}></input><br></br>
<input type="date" name="dob"{...register("dob")}></input><br></br>
<input type="number" name="mark" placeholder='enter mark' {...register("mark")}></input><br></br>
<input type="radio" name="gender" value="Male" {...register("gender")}></input>Male<br></br>
<input type="radio" name="gender" value="Female" {...register("gender")}></input>Female<br></br>
<input type="radio" name="gender" value="Others" {...register("gender")}></input>Others<br></br>
<input type="submit" value="clickme"></input><br></br>
<div id="r1"></div>
<div id="r2"></div>
<div id="r3"></div>
<div id="r4"></div>
<div id="r5"></div>
  </form>
  </>
)
}
export default App

