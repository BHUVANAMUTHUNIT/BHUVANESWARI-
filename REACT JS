EVENT TARGET NAME:
=================
function App()
{
  const show=(event)=>{
    if(event.target.name==="rno")
    document.getElementById("r1").innerHTML=event.target.value
    if(event.target.name==="name")
    document.getElementById("r2").innerHTML=event.target.value
    if(event.target.name==="mark")
    document.getElementById("r3").innerHTML=event.target.value
  }
  return(
    <>
    <h1>Event target---name</h1>
      <input type="text"  onChange={(e)=>show(e)} name="rno" placeholder="enter rno"></input><br></br>
      <input type="text"  onChange={(e)=>show(e)} name="name" placeholder="enter name"></input><br></br>
      <input type="text"  onChange={(e)=>show(e)} name="mark" placeholder="enter mark"></input><br></br>
      <div id="r1"></div>
      <div id="r2"></div>
      <div id="r3"></div>
    </>
  )
}
export default App

EVENT TARGET-TAGNAME :
====================
function App()
{
  const show=(event)=>{
    if(event.target.tagName==="INPUT")
    document.getElementById("r1").innerHTML="you are typing in textbox..."
    else if(event.target.tagName==="TEXTAREA")
    document.getElementById("r1").innerHTML="you are typing in addressbox..."
    else if(event.target.tagName==="SELECT")
    document.getElementById("r1").innerHTML="you are changing month..."
    else if(event.target.tagName==="BUTTON")
    document.getElementById("r1").innerHTML="you are clicking button..."
  }
  return(
    <>
    <h1>Event target--tagname</h1>
    <input type="text" onChange={(e)=>show(e)} placeholder="enter username:"></input><br></br>
    Enter Address:<textarea row="5" cols="25" onChange={(e)=>show(e)}></textarea><br></br>
    <select onChange={(e)=>show(e)}>
    <option>January</option>
    <option>February</option>
    <option>March</option>
    <option>April</option>
    <option>May</option>
    <option>June</option>
    <option>July</option>
    <option>September</option>
    <option>October</option>
    <option>November</option>
    <option>December</option>
    </select>
    <button onClick={(e)=>show(e)}>clickme</button>
    <div id="r1"></div>
    </>
  )
}
export default App

EVENT TARGET TYPE:
=================
function App()
{
  const show=(event)=>{
    if(event.target.type==="text")
    document.getElementById("r1").innerHTML="you are typing textbox..."
    else if(event.target.type==="password")
    document.getElementById("r1").innerHTML="you are typing password..."
    else if(event.target.type==="email")
    document.getElementById("r1").innerHTML="you are typing email..."
    else if(event.target.type==="date")
    document.getElementById("r1").innerHTML="you are changing date..."
    else if(event.target.type==="week")
    document.getElementById("r1").innerHTML="you are changing week..."
    else if(event.target.type==="month")
    document.getElementById("r1").innerHTML="you are changing month..."
    else if(event.target.type==="textarea")
    document.getElementById("r1").innerHTML="you are typing in address box..."
    else if(event.target.type==="radio")
    document.getElementById("r1").innerHTML="you are selecting gender..."
    else if(event.target.type==="checkbox")
    document.getElementById("r1").innerHTML="you are selecting courses..."
    else if(event.target.type==="number")
    document.getElementById("r1").innerHTML="you are typing a number..."
    else if(event.target.type==="time")
    document.getElementById("r1").innerHTML="you are changing time..."
    else if(event.target.type==="button")
    document.getElementById("r1").innerHTML="you are clicking a button..."
    else
    document.getElementById("r1").innerHTML="Others"
  }
  return(
    <>
    <h1>Event target--type</h1>
    USERNAME:<input type="text" onChange={(e)=>show(e)} placeholder="enter username"></input><br></br>
    PASSWORD:<input type="password" onChange={(e)=>show(e)} placeholder="enter password"></input><br></br>
    EMAIL:<input type="email" onChange={(e)=>show(e)} placeholder="enter email"></input><br></br>
    DATE:<input type="date" onChange={(e)=>show(e)} placeholder="enter date"></input><br></br>
    {/*WEEK:<select onChange={(e)=>show(e)}>
      <option>Sunday</option>
      <option>Monday</option>
      <option>Tuesday</option>
      <option>Wednesday</option>
      <option>Thursday</option>
      <option>Friday</option>
      <option>Saturday</option>
      </select>*/}
  <br></br>
    ENTER ADDRESS:<textarea row="5" cols="25" onChange={(e)=>show(e)}></textarea><br></br>
    {/*MONTH:<select type="select" onChange={(e)=>show(e)}>
    <option>January</option>
    <option>February</option>
    <option>March</option>
    <option>April</option>
    <option>May</option>
    <option>June</option>
    <option>July</option>
    <option>September</option>
    <option>October</option>
    <option>November</option>
    <option>December</option>
    </select>*/}
    <br></br>
    SELECT GENDER:<input type="radio" onChange={(e)=>show(e)}></input>Male
    <input type="radio" onChange={(e)=>show(e)}></input>FeMale
    <input type="radio" onChange={(e)=>show(e)}></input>Others
