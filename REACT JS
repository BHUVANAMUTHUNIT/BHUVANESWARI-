DELETED ID 1:
============
import React,{useState} from 'react'
import Axios from 'axios'
function App()
{
      const[list,setList]=React.useState(null)
      const[rno,setRno]=useState(0)
      const callaxios=()=>{
        Axios.get(`http://127.0.0.1:4003/api/bhuvana`,{params:{rno:rno}}).then((res)=>{
               setList(res.data)
               console.log("final res",res.data)
          }).catch((err)=>{
               console.log(err)
          })
     }
       const changeRno=(event)=>{
           setRno(event.target.value)
      }
      return(
        <>
             <h1>welcome to React component</h1>
           <input type="text" placeholder="enter rno" value={rno} onChange={(e)=>changeRno(e)}></input><br></br>
           <button onClick={callaxios}>clickme</button>
          <br></br>
          {list!==null && list.deletedCount>0 &&
               <>
                 <h1>successfully deleted</h1>
               </>
               
          }
          {list!==null && list.deletedCount===0 &&
          <>
          <h1>Not deleted!!! pls check rno or rno not found in db</h1>
          </>
          }
          
       </>
  )
}
export default App

5.UPDATED ID:
============
import React,{useState} from 'react'
import Axios from 'axios'
import {FontAwesomeIcon} from '@fortawesome/react-fontawesome'
import { } from '@fortawesome/fontawesome-svg-core'
import {faMagnifyingGlassLocation} from '@fortawesome/free-solid-svg-icons'
function App()
{
      const[list,setList]=React.useState(null)
      const[rno,setRno]=useState()
      const[name,setName]=useState()
      const[mark,setMark]=useState()
      const[readOnly,setReadOnly]=useState(true)
      const callaxios=()=>{
        Axios.get(`http://127.0.0.1:4004/api/bhuvana`,{params:{rno:rno}}).then((res)=>{
               setList(res.data)
               console.log("final res",res.data)
               //document.getElementById("u").value=res.data.name
               //document.getElementById("k").value=res.data.mark
            //  if(readOnly===true) 
            //  document.getElementById("re").value=res.data
            // else
            // document.getElementById("re").value=" " 
            setName(res.data.name)
            setMark(res.data.mark)
          }).catch((err)=>{
               console.log(err) 
          })
     }
     const updates=()=>{
      Axios.get(`http://127.0.0.1:4005/api/bhuvana`,{params:{rno:rno,name:name,mark:mark}}).then((res)=>{
             setList(res.data)
             console.log("final res",res.data)
            }).catch((err)=>{
              console.log(err) 
         })
    }
     const enable=()=>{
      setReadOnly(!readOnly)
     }
     
     
       const changeRno=(event)=>{
           setRno(event.target.value)
      }
            const changeName=(event)=>{
           setName(event.target.value)
      }
      const changeMark=(event)=>{
           setMark(event.target.value)
      }
      // const changeEnable=(event)=>{
        // setEnable(event.target.value)
      // }
      return(
        <>
             <h1>welcome to React component</h1>
           <input type="text" placeholder="enter rno" value={rno} onChange={(e)=>changeRno(e)}></input>
           <FontAwesomeIcon icon={faMagnifyingGlassLocation} size='1x' style={{color:"blue"}} onClick={callaxios} value="search"></FontAwesomeIcon><br></br>
           <input type="text" placeholder="enter rno"  id="u" value={name} onChange={(e)=>changeName(e)} readOnly={readOnly}></input><br></br>
           <input type="text" placeholder="enter rno" id="k" value={mark} onChange={(e)=>changeMark(e)} readOnly={readOnly}></input><br></br>
           {/* <button onClick={callaxios}>search</button> */}
            <button onClick={enable}>enable</button>
            <button onClick={updates}>update</button>  
           {/* <button>search</button> */}
          <br></br>
          {list!==null && list.deletedCount>0 &&
               <>
                 <h1>successfully deleted</h1>
               </>
               
          }
          {list!==null && list.deletedCount===0 &&
          <>
          <h1>Not deleted!!! pls check rno or rno not found in db</h1>
          </>
          }
          
       </>
  )
}
export default App

