REACTJS SIMPLE PROGRAM:
======================
function App()
{
  var a=10
  var b=20
  var c=a+b
  return( 
    <div>
      <h1>A value:{a}</h1>
      <h1>B value:{b}</h1>
      <h1> Total: {c}</h1>
        <h1>Welcome to React</h1>
        <h2>this is header 2</h2>
        <h3>this is header 3</h3>
        </div>
        )
}
export default App

