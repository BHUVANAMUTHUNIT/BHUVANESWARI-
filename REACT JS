REACT HOOKS ERROR HANDLING:
==========================
import {useForm} from 'react-hook-form'
function App()
{
  const {register,handleSubmit,formState:{errors}}=useForm()
  const dis=(data)=>{
    console.log(data)
  }
return(
  <>
  <form onSubmit={handleSubmit(dis)}>
  <h1>Error Handling</h1>
  <input type="text" name="un" placeholder='enter user name' {...register("un",{required:true,pattern:/^[a-z]+$/})}></input>
  {errors.un && errors.un.type==="required" && <font color="red">* must fill user name </font>}
  {errors.un && errors.un.type==="pattern" && <font color="red">* only type small a-z</font>}<br></br>
  <input type="text" name="cap" placeholder='enter user name' {...register("cap",{required:true,pattern:/^[A-Z]+$/})}></input>
  {errors.cap && errors.cap.type==="required" && <font color="red">* must fill user name </font>}
  {errors.cap && errors.cap.type==="pattern" && <font color="red">* only type capital A-Z</font>}<br></br>
  {/*<input type="text" name="un" placeholder='enter user name' {...register("un",{required:true,pattern:/^[a-z]{5}$/})}></input>
  {errors.un && errors.un.type==="required" && <font color="red">* must fill user name </font>}
  {errors.un && errors.un.type==="pattern" && <font color="red">* only type small a-z</font>}<br></br>
  <input type="text" name="un" placeholder='enter user name' {...register("un",{required:true,pattern:/^[A-Z]{5}$/})}></input>
  {errors.un && errors.un.type==="required" && <font color="red">* must fill user name </font>}
  {errors.un && errors.un.type==="pattern" && <font color="red">* only type capital A-Z</font>}<br></br>
  <input type="number" name="num" placeholder='enter number' {...register("num",{required:true,pattern:/^[0-9]{10}$/})}></input>
  {errors.num && errors.num.type==="required" && <font color="red">* must fill numbers</font>}
  {errors.num && errors.num.type==="pattern" && <font color="red">* only type numbers 0-9 / only 10 numbers</font>}<br></br>
  <input type="text" name="spl" placeholder='enter spl char' {...register("spl",{required:true,pattern:/^[^a-zA-Z0-9]+$/})}></input>
  {errors.spl && errors.spl.type==="required" && <font color="red">* must fill numbers</font>}
  {errors.spl && errors.spl.type==="pattern" && <font color="red">* only spl char</font>}<br></br>
  <input type="password" name="pw" placeholder='enter password' {...register("pw",{required:true,minLength:8,maxLength:12})}></input>
  {errors.pw && errors.pw.type==="required" && <font color="red">* must fill password </font>}
  {errors.pw && errors.pw.type==="minLength" && <font color="red">* min char: 8 need </font>}
  {errors.pw && errors.pw.type==="maxLength" && <font color="red">* max char: 12 exceeded </font>}<br></br>
  <input type="submit" value="clickme"></input><br></br>
  </form>
  </>
)
}
export default App

REACT LIFECYCLE METHOD-MOUNTING (COMPONENTS):
===============================
import React from 'react'
class App extends React.Component
{
  constructor(props)  //before render one time callimg
  {
    super(props)
    this.state=this.props.data
    
  }
  render()    //always calling
  {
  return(
    <>
       <h1>React lifecycle components</h1>
       <h1>Type 1: Mounting (i.constructor ii.render() iii.ComponentDidMount</h1>
        <h1>RNO:{this.state.rno}</h1>
        <h2>SNAME:{this.state.name}</h2>
        <h2>MARK:{this.state.mark}</h2>
        <div id="res1"></div>
        <div id="res2"></div>
    </>
  )
}
shouldComponentUpdate()  //after render
{
return true
}
getSnapshotBeforeUpdate(prevProps,prevState)  //after should component update + true
{
  document.getElementById("res1").innerHTML="<h1> Old Values:</h1><h2>Rollno:"+prevState.rno+"<br>Name:"+prevState.name+"<br>Mark:"+prevState.mark+"</h2>"
}
componentDidMount()  //after should component update + return must be true
{
  setTimeout(()=>{
    this.setState({mark:300,name:"muthu",rno:56})
  },5000)
}
componentDidUpdate()    //after component did mount
{
  document.getElementById("res2").innerHTML="<h1>update success</h1>"
}
}
export default App

HTTP AND AXIOS:
==============
import axios from "axios"
//import './App.css'
import {useState} from 'react'
function App()
{
  const [city,setCity]=useState("")
  const[output,setOutput]=useState(null)
  const updatecity=(event)=>{
    setCity(event.target.value)
  }
  const findreport=()=>{
    document.getElementById("res1").innerHTML=""
    const wapi=`https://api.openweathermap.org/data/2.5/weather?q=${city}&appid=8a0f210b4a896240d0350d0fa4616b8f`
  console.log(wapi)
  if(city.length>2)
  {
    axios.get(wapi).then((res)=>{
      console.log(res.data)
      setOutput(res.data)
    }).catch((err)=>{
      console.log(err.response.data)
      setOutput(err.response.data)
    }).finally(()=>{
      document.getElementById("res2").innerHTML="====Report End===="
    })
  }
  else
  {
    document.getElementById("res1").innerHTML="city name atleast 3 chars"
  }
  }
return(
  <>
  <h1>API handling using React and Axios HTTP header</h1>
  <input type="text" onChange={(e)=>updatecity(e)} value={city} placeholder="Enter City Name:"></input><br></br>
  <input type="button" onClick={findreport} value="weather report"></input><br></br> 
  <div id="res1"></div>
  {output!==null && output.cod===200 && <>
  <h2>Weather Report1:{output.weather[0].main}</h2>
  <h2>Weather Report2:{output.weather[0].description}</h2>
  <h2>Temperature:{output.main.temp}</h2>
  </>}
  {output!==null && output.cod==="404" && <>
  <h2>Result:{output.message}</h2>
  </>}
  <div id="res2"></div>
  </>
)
}
export default App

REACT LIFECYCLE METHOD-UNMOUNTING:
=================================
import React from "react"
import{FontAwesomeIcon} from '@fortawesome/react-fontawesome'
//import {} from '@fontawesome-svg-core'
import{faGear,faEgg,faHome} from '@fortawesome/free-solid-svg-icons'
class Child extends React.Component
{
  componentWillUnmount()
  {
    document.getElementById("res").innerHTML="<h1>Child Component Removed</h1>"
  }
  render()
  {
    return(
      <>
      <h1>This is child component</h1>
      <FontAwesomeIcon icon={faGear} spin></FontAwesomeIcon>
      <FontAwesomeIcon icon={faEgg} spin></FontAwesomeIcon>
      <FontAwesomeIcon icon={faHome} size="lg" style={{color:"red"}} spin></FontAwesomeIcon>
      </>
    )
  }
}
class App extends Child
{
  constructor()
  {
    super()
    this.state={
      show:true
    }
  }
  render()
  {
    var com
    const dcomponent=()=>{
      this.setState({show:false})
    }
    if(this.state.show===true)
    {
       com=<Child/>
    }
    return(
      <>
      <h1>Unmounting - Deleted Component</h1>
      <br></br>
      {com}
      <br></br>
      <button onClick={dcomponent}>Child Component Removed</button>
      <div id="res"></div>
    
      </>
    )
  }
}
export default App

WEATHER REPORT:
==============
import axios from "axios"
import './App.css'
import {useState} from 'react'
function App()
{
  const [city,setCity]=useState("")
  const[output,setOutput]=useState(null)
  const updatecity=(event)=>{
    setCity(event.target.value)
  }
  const findreport=()=>{
    document.getElementById("res1").innerHTML=""
    const wapi=`https://api.openweathermap.org/data/2.5/weather?q=${city}&appid=8a0f210b4a896240d0350d0fa4616b8f`
  console.log(wapi)
  if(city.length>2)
  {
    axios.get(wapi).then((res)=>{
      console.log(res.data)
      setOutput(res.data)
    }).catch((err)=>{
      console.log(err.response.data)
      setOutput(err.response.data)
    }).finally(()=>{
      document.getElementById("res2").innerHTML="====Report End===="
    })
  }
  else
  {
    document.getElementById("res1").innerHTML="city name atleast 3 chars"
  }
  }
return(
    <>
    
    <h1>WEATHER REPORT</h1>
    <input type="text" onChange={(e)=>updatecity(e)} value={city} placeholder="Enter City Name"></input>
    <input type="button" onClick={findreport} value="Search"></input>
     <div id="res1"></div>
  {output!==null && output.cod===200 && <>
  <h2>Weather Report1:{output.weather[0].main}</h2>
  <h2>Weather Report2:{output.weather[0].description}</h2>
  <h2>Temperature:{output.main.temp}</h2>
  </>}
  {output!==null && output.cod==="404" && <>
  <h2>Result:{output.message}</h2>
  </>}
  <div id="res2"></div>
    </div>
  </>
  )
}
export default App

CONNECTION TO FRONTEND AND BACKEND: (APP.JS)
==================================
1.MONGOBD WITH NODEJS AND REACTJS:
=================================
import React,{ useEffect } from 'react'
import Axios from 'axios'
function App()
{
  const [list,setList]=React.useState([])
  useEffect(()=>{
    Axios.get("http://127.0.0.1:4000/api/gowtham").then((res)=>{
    setList(res.data)
    })
  },[])
  
  return(
    <>
      <h1>Welcome to React Component</h1>
      // <ol>
      // {list.map((item)=><li> {item.rno} {item.name} {item.mark} </li>)}
      // </ol>
      <table border="2px"> 
      {list.map((item)=><tr><td>{item.rno}</td> <td>{item.name}</td> <td>{item.mark}</td></tr>)}
      </table>
    </>
  )
}
export default App

2. MONGOBD WITH NODEJS AND REACTJS 1:
====================================
import React,{ useState } from 'react'
//import Axios from 'axios'
import axios from 'axios'
function App()
{
  const [list,setList]=React.useState([])
  const[rno,setRno]=useState(null)
  //useEffect(()=>{
    //Axios.get("http://127.0.0.1:4000/api/gowtham").then((res)=>{
    //setList(res.data)
    //})
  //},[])
  const callaxios=()=>{
    //axios.get(`http://127.0.0.1:4001/api/bhuvana/${rno}`).then((res)=>{
      axios.get(`http://127.0.0.1:4001/api/bhuvana`,{params:{myrno:rno,myname:"bhu"}}).then((res)=>{
      setList(res.data)
    }).catch((err)=>{
      console.log(err)
    })
  }
  const changeRno=(event)=>{
    setRno(event.target.value)
  }
     
  return(
    <>
      <h1>Welcome to React Component</h1>
      <input type="text" placeholder='enter rno:' value={rno} onChange={(e)=>changeRno(e)}></input>
      <button onClick={callaxios}>clickme</button>
      <br></br>
      
        {list.length>0 && <>
       <table border="2px"> 
      {list.map((item)=><tr><td>{item.rno}</td> <td>{item.name}</td> <td>{item.mark}</td></tr>)}
      </table>
    </>
}
{list.length===0 && <>
<h1>NOT FOUND</h1>
</>}
</>
  )
}
export default App

3.INSERTING ID:
===============
import React,{useState} from 'react'
import Axios from 'axios'
function App()
{
      const[list,setList]=React.useState(null)
      const[rno,setRno]=useState(0)
      const[name,setName]=useState("")
      const[mark,setMark]=useState(0)
     //  useEffect(()=>{
     //       Axios.get('http://127.0.0.1:4000/api/bhuvana').then((res)=>{
     //       setList(res.data)
     //       })

     //  },[])
     //
     const callaxios=()=>{
         // Axios.get(`http://127.0.0.1:4001/api/bhuvana/${rno}`).then((res)=>{
          Axios.get(`http://127.0.0.1:4002/api/bhuvana`,{params:{myrno:rno,myname:name,mymark:mark}}).then((res)=>{
               setList(res.data)
               console.log("final res",res.data)
          }).catch((err)=>{
               console.log(err)
          })
     }
       const changeRno=(event)=>{
           setRno(event.target.value)
      }
      const changeName=(event)=>{
           setName(event.target.value)
      }
      const changeMark=(event)=>{
           setMark(event.target.value)
      }
  return(
       <>
            <h1>welcome to React component</h1>
          <input type="text" placeholder="enter rno" value={rno} onChange={(e)=>changeRno(e)}></input><br></br>
          <input type="text" placeholder="enter name" value={name} onChange={(e)=>changeName(e)}></input><br></br>
          <input type="text" placeholder="enter mark" value={mark} onChange={(e)=>changeMark(e)}></input><br></br>
          <button onClick={callaxios}>clickme</button>
          <br></br>
          {list!==null && <>
               object id:{list.insertedId}
               <br></br>
               acknowledged:{list.acknowledged}
          </>}
       </>
  )
}
export default App

