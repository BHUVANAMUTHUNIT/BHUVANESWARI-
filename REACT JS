=======================
function App()
{
  var list=[{rno:1,name:'bhuvana'},{rno:2,name:'muthu'},{rno:3,name:'nithu'}]
  return(
    <>
    <h1>React json with array</h1>
  <o1>{list.map((item)=><li>{item.rno} {item.name}</li>)}</o1>
    </>
    )
  }
  export default App

