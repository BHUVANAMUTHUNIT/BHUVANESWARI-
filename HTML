	500*<input type="text" name="t2"><br>
    200*<input type="text" name="t3"><br>
    100*<input type="text" name="t4"><br>
    50*<input type="text" name="t5"><br>
    5*<input type="text" name="t6"><br>


		<output name="ans"></output>
		
	</form>

