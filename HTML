SELECT OPTIONS:
===============
<!DOCTYPE html>
<html>
<head>
	<title>select option</title>
</head>
<body>
	<form>
		<select name="m">
			<option> January</option>
			<option> February</option>
			<option> March</option>
			<option> April</option>
			<option> May</option>
			<option> June</option>
			<option> July</option>
			<option> August</option>
			<option> September</option>
			<option> October</option>
			<option> November</option>
			<option> December</option>
			
		</select>
